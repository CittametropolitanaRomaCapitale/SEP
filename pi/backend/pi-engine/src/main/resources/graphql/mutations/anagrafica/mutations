---------INSERT CONTATTO IN ANAGRAFICA-------------------------
mutation saveContatto($anagraficaInput: AnagraficaInputInput) {
  saveContatto(anagraficaInput: $anagraficaInput) {
    ragioneSociale
    nome
    cognome
    cfPiva
    indirizzo
    citta
    cap
    provincia
    email
    pec
    telefono
    fax
    note
    tsCreation
    tsStartVali
    cancellato
  }
}
