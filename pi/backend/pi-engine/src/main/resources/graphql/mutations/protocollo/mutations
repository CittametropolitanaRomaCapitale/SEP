---------------------------------------------------------------------
- save new PE configuration - MUTATION

mutation savePE($input: PecPeoDTOInputInput) {
  saveConfigurazionePE(pecPeoDTOInput: $input) {
    id
  }
}

save new PE configuration - VARIABLES
{
  "input": {
    "idUtente": "1",
    "tipologiaPosta": "PEC",
    "cdr": "AVV0000",
    "indirizzoEmail": "lorenz@info",
    "username": "user",
    "password": "password"
  }
}

-------------------------------------------

mutation UpdateProtocollo($input: ProtocolloUpdateInput!) {
  updateProtocollo(input: $input) {
    nProtocollo
    note
    nProtocolloCircolare
    allegati {
      id
      nome
      oggetto
      dimensione
      estensione
      isMain
      collocazioneTelematica
    }
  }
}