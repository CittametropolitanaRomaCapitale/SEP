        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">

    <changeSet id="add_columns_to_registro_giornaliero" author="Andrea De Matteis">
        <addColumn tableName="registro_giornaliero">
            <column name="esito_versamento" type="varchar(255)">
