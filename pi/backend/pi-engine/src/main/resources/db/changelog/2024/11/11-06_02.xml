<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">

    <changeSet id="alter_table_notifiche_id_protocollo" author="Gioacchino De Fusco">
        <modifyDataType tableName="protocollo_notifiche" columnName="id_protocollo" newDataType="BIGINT"/>
    </changeSet>
    <changeSet id="insert_configurazione_globale_01" author="Gioacchino De Fusco">
        <insert tableName="configurazioni">
            <column name="id" value="5" />
            <column name="nome" value="basePublicUrl" />
            <column name="valore" value="http://127.0.0.1:3000" />
            <column name="tipo" value="url" />
            <column name="categoria" value="global" />
        </insert>
        <insert tableName="configurazioni">
            <column name="id" value="6" />
            <column name="nome" value="applicationId" />
            <column name="valore" value="3" />
            <column name="tipo" value="integer" />
            <column name="categoria" value="global" />
        </insert>
    </changeSet>

</databaseChangeLog>


