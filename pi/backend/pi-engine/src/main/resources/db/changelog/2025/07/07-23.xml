<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">

    <changeSet id="add-emergency-number-protocollo-column-to-protocollo" author="michela carnevale">
        <addColumn tableName="PROTOCOLLI">
            <column name="numero_emergenza" type="VARCHAR(255)"/>
        </addColumn>
    </changeSet>

    <changeSet id="add-emergency-date-protocollo-column-to-protocollo" author="michela carnevale">
        <addColumn tableName="PROTOCOLLI">
            <column name="data_emergenza" type="TIMESTAMP"/>
        </addColumn>
    </changeSet>

</databaseChangeLog>