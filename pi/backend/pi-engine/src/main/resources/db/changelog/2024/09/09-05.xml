<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">


    <changeSet id="add_column_id_titolario_to_storico" author="Canzaniello Francesco">

        <addColumn tableName="STORICO">
            <column name="ID_TITOLARIO" type="BIGINT">
                <constraints nullable="true"/>
            </column>
        </addColumn>


        <addForeignKeyConstraint baseTableName="STORICO"
                                 baseColumnNames="ID_TITOLARIO"
                                 constraintName="fk_id_titolario"
                                 referencedTableName="TITOLARIO"
                                 referencedColumnNames="ID"/>
    </changeSet>

</databaseChangeLog>


